r_idx = np.argsort(sim_scores)   # sort and get sorted index\n","\n","        sim_scores = sim_scores[user_idx][-neighbor_size:]  # sim scores as much as user_idx size\n","\n","        # 영화 rating을 neighbor size만큼 받기\n","        movie_ratings = movie_ratings[user_idx][-neighbor_size:]\n","\n","        # 최종 예측값 계산 \n","        mean_rating = np.dot(sim_scores, movie_ratings) / sim_scores.sum()\n","    else:\n","        mean_rating = 3.0\n","\n","    return mean_rating"],"metadata":{"id":"LZSDENQUyqDO","executionInfo":{"status":"ok","timestamp":1648338156402,"user_tz":-540,"elapsed":237,"user":{"displayName":"Roy Son","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GhVMzhGmOLO-LtCUnDj98IHVt9qwUVSM38Ow5VCJA=s64","userId":"14004762039756736463"}}},"execution_count":183,"outputs":[]},{"cell_type":"code","source":["score(cf_knn, neighbor_size=30)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"GSH_vtvFzHIl","executionInfo":{"status":"ok","timestamp":1648338195127,"user_tz":-540,"elapsed":32631,"user":{"displayName":"Roy Son","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GhVMzhGmOLO-LtCUnDj98IHVt9qwUVSM38Ow5VCJA=s64","userId":"14004762039756736463"}},"outputId":"cba4f0eb-4fdf-4c8f-93c6-6a30964fd879"},"execution_count":184,"outputs":[{"output_type":"execute_result","data":{"text/plain":["1.0099259423351494"]},"metadata":{},"execution_count":184}]},{"cell_type":"code","source":["def recom_movie(user_id, n_items, neighbor_size=30):\n","    user_movie = rating_matrix.loc[user_id].copy()\n","    for movie in rating_matrix:        \n","        user_movie.loc[movie] = cf_knn(user_id, movie, neighbor_size)\n","\n","    movie_sort = user_movie.sort_values(ascending=False)[:n_items]\n","    recom_movies = movies.loc[movie_sort.index]\n","    recommendations = recom_movies['title']\n","    return recommendations"],"metadata":{"id":"CHoTsNxO3sGd","executionInfo":{"status":"ok","timestamp":1648338202117,"user_tz":-540,"elapsed":437,"user":{"displayName":